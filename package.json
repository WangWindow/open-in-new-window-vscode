{
	"name": "quick-open-in-new-window",
	"displayName": "%command.open.titleen%",
	"description": "%command.open.title%",
	"version": "1.1.1",
	"publisher": "WangWindow",
	"bugs": {
		"url": "https://github.com/WangWindow/open-in-new-window-vscode/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/WangWindow/open-in-new-window-vscode"
	},
	"author": "WangWindow",
	"license": "MIT",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"open",
		"window"
	],
	"icon": "assets/logo.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "quick-open-in-new-window.open",
				"title": "%command.open.title%"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "quick-open-in-new-window.open",
					"group": "openInNewWindowGroup"
				}
			]
		},
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"quick-open-in-new-window.showNotification": {
					"type": "boolean",
					"description": "%configuration.showNotification%",
					"default": false
				}
			}
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"package": "vsce package",
		"publish": "vsce publish",
		"prepublishOnly": "npm run compile"
	},
	"devDependencies": {
		"@types/vscode": "^1.75.0",
		"@vscode/vsce": "^3.7.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@vscode/l10n": "^0.0.18"
	}
}
